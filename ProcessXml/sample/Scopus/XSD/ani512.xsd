<!--
     Typical invocation:
    
     <bibdataset xmlns="http://www.elsevier.com/xml/ani/ani"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:ce="http://www.elsevier.com/xml/ani/common"
         xmlns:ait="http://www.elsevier.com/xml/ani/ait"
         xsi:schemaLocation="http://www.elsevier.com/xml/ani/ani http://www.elsevier.com/xml/ani/ani512.xsd">
-->

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified"
    targetNamespace="http://www.elsevier.com/xml/ani/ani" xmlns:ait="http://www.elsevier.com/xml/ani/ait"
    xmlns:ani="http://www.elsevier.com/xml/ani/ani" xmlns:ce="http://www.elsevier.com/xml/ani/common">

    <xs:import namespace="http://www.elsevier.com/xml/ani/ait" schemaLocation="ait.xsd"/>
    <xs:import namespace="http://www.elsevier.com/xml/ani/common" schemaLocation="ce.xsd"/>
    <xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml.xsd"/>
    <xs:include schemaLocation="ani-ce.xsd"/>

    <!-- ========== Citation types ==========
        ab  Abstract Report
        ar  Article
        ba
        bk  Book
        br  Book Review
        bz  Business Article
        cb  Conference Abstract
        ch  Chapter
        cp  Conference Paper
        cr  Conference Review
        di  Dissertation
        ed  Editorial
        er  Erratum
        ip  Article in Press
        le  Letter
        no  Note
        pa  Patent
        pr  Press Release
        re  Review
        rf
        rp  Report
        sh  Short Survey
        wp  Working Paper
    -->
    <xs:simpleType name="citationtype.att">
        <xs:restriction base="xs:token">
            <xs:enumeration value="ab"/>
            <xs:enumeration value="ar"/>
            <xs:enumeration value="ba"/>
            <xs:enumeration value="bk"/>
            <xs:enumeration value="br"/>
            <xs:enumeration value="bz"/>
            <xs:enumeration value="cb"/>
            <xs:enumeration value="ch"/>
            <xs:enumeration value="cp"/>
            <xs:enumeration value="cr"/>
            <xs:enumeration value="di"/>
            <xs:enumeration value="ed"/>
            <xs:enumeration value="er"/>
            <xs:enumeration value="ip"/>
            <xs:enumeration value="le"/>
            <xs:enumeration value="no"/>
            <xs:enumeration value="pa"/>
            <xs:enumeration value="pr"/>
            <xs:enumeration value="re"/>
            <xs:enumeration value="rf"/>
            <xs:enumeration value="rp"/>
            <xs:enumeration value="sh"/>
            <xs:enumeration value="wp"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- ========== GenCitation types ==========
        bk  Book
        ch  Chapter
        co  Conference Proceeding
        cp  Conference Paper
        di  Dissertation
        ep  E-print
        ot  Other
        pa  Patent
        st  Standard
        tr  Technical Report
    -->
    <xs:simpleType name="gencitationtype.att">
        <xs:restriction base="xs:token">
            <xs:enumeration value="bk"/>
            <xs:enumeration value="ch"/>
            <xs:enumeration value="co"/>
            <xs:enumeration value="cp"/>
            <xs:enumeration value="di"/>
            <xs:enumeration value="ep"/>
            <xs:enumeration value="ot"/>
            <xs:enumeration value="pa"/>
            <xs:enumeration value="st"/>
            <xs:enumeration value="tr"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- ========== Sequencebank types ==========
    -->
    <xs:simpleType name="sequencebanktype.att">
        <xs:restriction base="xs:token">
            <xs:enumeration value="DDBJ"/>
            <xs:enumeration value="EMBL"/>
            <xs:enumeration value="GENBANK"/>
            <xs:enumeration value="PIR"/>
            <xs:enumeration value="SWISSPROT"/>
            <xs:enumeration value="UNNAMED"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- ========== Descriptor types ==========
    -->
    <xs:simpleType name="descriptortype.att">
        <xs:restriction base="xs:token">
            <xs:enumeration value="AUD"/>
            <xs:enumeration value="AUT"/>
            <xs:enumeration value="BLS"/>
            <xs:enumeration value="BTC"/>
            <xs:enumeration value="CBA"/>
            <xs:enumeration value="CBB"/>
            <xs:enumeration value="CBC"/>
            <xs:enumeration value="CBE"/>
            <xs:enumeration value="CCV"/>
            <xs:enumeration value="CFL"/>
            <xs:enumeration value="CLU"/>
            <xs:enumeration value="CMH"/>
            <xs:enumeration value="CNC"/>
            <xs:enumeration value="CSL"/>
            <xs:enumeration value="CSS"/>
            <xs:enumeration value="CSW"/>
            <xs:enumeration value="CTB"/>
            <xs:enumeration value="CTC"/>
            <xs:enumeration value="CTN"/>
            <xs:enumeration value="DID"/>
            <xs:enumeration value="DRA"/>
            <xs:enumeration value="DRG"/>
            <xs:enumeration value="ECN"/>
            <xs:enumeration value="FDE"/>
            <xs:enumeration value="GDE"/>
            <xs:enumeration value="GLY"/>
            <xs:enumeration value="KWD"/>
            <xs:enumeration value="MDV"/>
            <xs:enumeration value="MEA"/>
            <xs:enumeration value="MED"/>
            <xs:enumeration value="MGS"/>
            <xs:enumeration value="MKW"/>
            <xs:enumeration value="MLA"/>
            <xs:enumeration value="MMS"/>
            <xs:enumeration value="MSC"/>
            <xs:enumeration value="MSF"/>
            <xs:enumeration value="MSH"/>
            <xs:enumeration value="MUD"/>
            <xs:enumeration value="OZR"/>
            <xs:enumeration value="PCV"/>
            <xs:enumeration value="REA"/>
            <xs:enumeration value="RGI"/>
            <xs:enumeration value="SID"/>
            <xs:enumeration value="SPC"/>
            <xs:enumeration value="SPO"/>
            <xs:enumeration value="SUA"/>
            <xs:enumeration value="TRP"/>
            <xs:enumeration value="WDE"/>
            <xs:enumeration value="SCO"/>
            <xs:enumeration value="SCT"/>
            <xs:enumeration value="IBT"/>
            <xs:enumeration value="IGI"/>
            <xs:enumeration value="SCC"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- ========== Classification types ==========
    -->
    <xs:simpleType name="classificationtype.att">
        <xs:restriction base="xs:token">
            <xs:enumeration value="ASJC"/>
            <xs:enumeration value="AUTARC-CONFIGURATION"/>
            <xs:enumeration value="CABSCLASS"/>
            <xs:enumeration value="CBNBGEO"/>
            <xs:enumeration value="CBNBSCOPE"/>
            <xs:enumeration value="CBNBSECTOR"/>
            <xs:enumeration value="CBNBSIC"/>
            <xs:enumeration value="CONFIGURATION-VALUE"/>
            <xs:enumeration value="CPXCLASS"/>
            <xs:enumeration value="EMCLASS"/>
            <xs:enumeration value="ESBD"/>
            <xs:enumeration value="ENCOMPASSCLASS"/>
            <xs:enumeration value="FLXCLASS"/>
            <xs:enumeration value="GEOCLASS"/>
            <xs:enumeration value="PATAUTO"/>
            <xs:enumeration value="PATENT-FAMILY"/>
            <xs:enumeration value="PATMANU"/>
            <xs:enumeration value="REAXYS-PRIORITY"/>
            <xs:enumeration value="SATURATION"/>
            <xs:enumeration value="SUBJECT"/>
            <xs:enumeration value="SUPPORT-INFORMATION"/>
            <xs:enumeration value="WTACLASS"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- ========== Tradename types ==========
    -->
    <xs:simpleType name="tradenametype.att">
        <xs:restriction base="xs:token">
            <xs:enumeration value="TNM"/>
            <xs:enumeration value="TNV"/>
            <xs:enumeration value="TNW"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- ========== Manufacturer types ==========
    -->
    <xs:simpleType name="manufacturertype.att">
        <xs:restriction base="xs:token">
            <xs:enumeration value="MNF"/>
            <xs:enumeration value="MNV"/>
            <xs:enumeration value="MNW"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- ========== Chemical source types ==========
    -->
    <xs:simpleType name="chemicalsourcetype.att">
        <xs:restriction base="xs:token">
            <xs:enumeration value="esbd"/>
            <xs:enumeration value="nlm"/>
            <xs:enumeration value="cbnb"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- ========== Source Types ==========
        b  Book
        d  Trade Journal
        j  Journal
        k  Book Series
        m  Multi-volume Reference Works
        p  Conference Proceeding
        r  Report
        n  Newsletter
        w  Newspaper
    -->
    <xs:simpleType name="sourcetype.att">
        <xs:restriction base="xs:token">
            <xs:enumeration value="b"/>
            <xs:enumeration value="d"/>
            <xs:enumeration value="j"/>
            <xs:enumeration value="k"/>
            <xs:enumeration value="m"/>
            <xs:enumeration value="p"/>
            <xs:enumeration value="r"/>
            <xs:enumeration value="n"/>
            <xs:enumeration value="w"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- ========== Contributor Roles ==========
        auth  author
        comp  compiler
        edit  editor
        illu  illustrator
        phot  photographer
        publ  publisher
        revi  reviewer
        tran  translator
    -->
    <xs:simpleType name="contributorrole.att">
        <xs:restriction base="xs:token">
            <xs:enumeration value="auth"/>
            <xs:enumeration value="comp"/>
            <xs:enumeration value="edit"/>
            <xs:enumeration value="illu"/>
            <xs:enumeration value="phot"/>
            <xs:enumeration value="publ"/>
            <xs:enumeration value="revi"/>
            <xs:enumeration value="tran"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="yesno.att">
        <xs:restriction base="xs:token">
            <xs:enumeration value="y"/>
            <xs:enumeration value="n"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:group name="pubdateType">
        <xs:sequence>
            <xs:element ref="ani:year" minOccurs="0"/>
            <xs:element ref="ani:month" minOccurs="0"/>
            <xs:element ref="ani:day" minOccurs="0"/>
            <xs:element ref="ani:date-text" minOccurs="0"/>
        </xs:sequence>
    </xs:group>
    <xs:element name="year" type="xs:string"/>
    <xs:element name="month" type="xs:string"/>
    <xs:element name="day" type="xs:string"/>
    <xs:element name="date-text" type="xs:string"/>

    <xs:complexType name="personalnameType">
        <xs:sequence>
            <xs:element minOccurs="0" ref="ce:initials"/>
            <xs:element ref="ce:indexed-name"/>
            <xs:choice minOccurs="0">
                <xs:sequence>
                    <xs:element minOccurs="0" ref="ce:degrees"/>
                    <xs:element ref="ce:surname"/>
                    <xs:element minOccurs="0" ref="ce:given-name"/>
                    <xs:element minOccurs="0" ref="ce:suffix"/>
                </xs:sequence>
                <xs:element ref="ani:nametext"/>
            </xs:choice>
        </xs:sequence>
    </xs:complexType>
    <xs:element name="nametext" type="xs:string"/>

    <xs:complexType name="affiliationType">
        <xs:choice>
            <xs:element ref="ce:text"/>
            <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="ani:organization"/>
                <xs:element minOccurs="0" ref="ani:address-part"/>
                <xs:choice minOccurs="0">
                    <xs:element ref="ani:city-group"/>
                    <xs:sequence>
                        <xs:element minOccurs="0" ref="ani:city"/>
                        <xs:element minOccurs="0" ref="ani:state"/>
                        <xs:element minOccurs="0" maxOccurs="unbounded" ref="ani:postal-code"/>
                    </xs:sequence>
                </xs:choice>
            </xs:sequence>
        </xs:choice>
    </xs:complexType>

    <xs:complexType name="classificationType" mixed="true">
        <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="ani:classification-code"/>
            <xs:element ref="ani:classification-description"/>
        </xs:choice>
        <xs:attribute name="weight">
            <xs:simpleType>
                <xs:restriction base="xs:token">
                    <xs:enumeration value="-"/>
                    <xs:enumeration value="?"/>
                    <xs:enumeration value="+"/>
                    <xs:enumeration value="++"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>

    <!-- ========== Wrapper and process info ========== -->

    <xs:element name="bibdataset">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="ani:item"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="item">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="ait:process-info"/>
                <xs:element ref="ani:bibrecord"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="bibrecord">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="ani:item-info"/>
                <xs:element minOccurs="0" ref="ani:itemfilelinks"/>
                <xs:element ref="ani:head"/>
                <xs:element minOccurs="0" ref="ani:tail"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- ========== Bibliographic record ========== -->

    <xs:element name="item-info">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="ani:copyright"/>
                <xs:element ref="ani:itemidlist"/>
                <xs:element ref="ani:history"/>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="ani:dbcollection"/>
                <xs:element minOccurs="0" ref="ani:external-source"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="itemfilelinks">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="ani:itemfilelink"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="itemfilelink">
        <xs:complexType>
            <xs:attribute name="href" use="required"/>
            <xs:attribute name="MIME-type"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="copyright">
        <xs:complexType mixed="true">
            <xs:attribute name="type"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="itemidlist">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="ce:pii"/>
                <xs:element minOccurs="0" ref="ce:doi"/>
                <xs:element maxOccurs="unbounded" ref="ani:itemid"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="itemid">
        <xs:complexType mixed="true">
            <xs:attribute name="idtype" type="xs:NMTOKEN" use="required"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="dbcollection" type="xs:string"/>

    <xs:element name="external-source" type="xs:string"/>

    <!-- Item history elements -->

    <xs:element name="history">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="ani:date-created"/>
                <xs:element minOccurs="0" ref="ani:date-completed"/>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="ani:date-revised"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="date-created">
        <xs:complexType>
            <xs:attributeGroup ref="ce:date.att"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="date-completed">
        <xs:complexType>
            <xs:attributeGroup ref="ce:date.att"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="date-revised">
        <xs:complexType>
            <xs:attributeGroup ref="ce:date.att"/>
        </xs:complexType>
    </xs:element>

    <!-- ========== head ========== -->

    <xs:element name="head" type="ani:headType"/>

    <xs:complexType name="headType">
        <xs:sequence>
            <xs:element minOccurs="0" ref="ani:citation-info"/>
            <xs:element minOccurs="0" maxOccurs="unbounded" ref="ani:related-item"/>
            <xs:element minOccurs="0" ref="ani:citation-title"/>
            <xs:element minOccurs="0" maxOccurs="unbounded" ref="ani:author-group"/>
            <xs:element minOccurs="0" ref="ani:correspondence"/>
            <xs:element minOccurs="0" ref="ani:grantlist"/>
            <xs:element minOccurs="0" ref="ani:abstracts"/>
            <xs:element minOccurs="0" ref="ani:source"/>
            <xs:element minOccurs="0" ref="ani:enhancement"/>
        </xs:sequence>
    </xs:complexType>

    <!-- Citation elements -->

    <xs:element name="citation-info" type="ani:citationinfoType"/>

    <xs:complexType name="citationinfoType">
        <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" ref="ani:citation-type"/>
            <xs:element minOccurs="0" maxOccurs="unbounded" ref="ani:citation-language"/>
            <xs:element minOccurs="0" maxOccurs="unbounded" ref="ani:abstract-language"/>
            <xs:element minOccurs="0" ref="ani:patent"/>
            <xs:element minOccurs="0" ref="ani:author-keywords"/>
            <xs:element minOccurs="0" ref="ani:dummy-link"/>
            <xs:element minOccurs="0" ref="ani:figure-information"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="citation-type">
        <xs:complexType>
            <xs:attribute name="code" use="required" type="ani:citationtype.att"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="citation-language">
        <xs:complexType>
            <xs:attribute ref="xml:lang" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="abstract-language">
        <xs:complexType>
            <xs:attribute ref="xml:lang" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="author-keywords">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="ani:author-keyword"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="author-keyword">
        <xs:complexType mixed="true">
            <xs:group minOccurs="0" maxOccurs="unbounded" ref="ani:richstring.data"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="dummy-link">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="ani:gen-citationtype"/>
                <xs:element ref="ani:itemlink"/>
            </xs:sequence>
            <xs:attribute name="restricted-access" type="ani:yesno.att" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="gen-citationtype">
        <xs:complexType>
            <xs:attribute name="code" use="required" type="ani:gencitationtype.att"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="itemlink" type="xs:string"/>
    <xs:element name="figure-information" type="xs:string"/>

    <xs:element name="related-item">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="ce:pii"/>
                <xs:element minOccurs="0" ref="ce:doi"/>
                <xs:element minOccurs="0" ref="ani:citation-info"/>
                <xs:element minOccurs="0" ref="ani:citation-title"/>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="ani:contributor-group"/>
                <xs:element minOccurs="0" ref="ani:source"/>
            </xs:sequence>
            <xs:attribute name="type" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="citation-title">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="ani:titletext"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="titletext">
        <xs:complexType mixed="true">
            <xs:group minOccurs="0" maxOccurs="unbounded" ref="ani:richstring.data"/>
            <xs:attribute ref="xml:lang" default="eng"/>
            <xs:attribute name="original" type="ani:yesno.att"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="contributor-group">
        <xs:complexType>
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:element ref="ani:contributor"/>
                    <xs:element ref="ani:collaboration"/>
                </xs:choice>
                <xs:element minOccurs="0" ref="ani:et-al"/>
                <xs:element minOccurs="0" ref="ani:affiliation"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="contributor">
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="ani:personalnameType">
                    <xs:sequence>
                        <xs:element minOccurs="0" ref="ce:e-address"/>
                    </xs:sequence>
                    <xs:attribute name="role" use="required" type="ani:contributorrole.att"/>
                    <xs:attribute name="auid"/>
                    <xs:attribute name="seq" use="required"/>
                    <xs:attribute name="type"/>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

    <!-- AuthorGroup elements -->

    <xs:element name="author-group">
        <xs:complexType>
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:element ref="ani:author"/>
                    <xs:element ref="ani:collaboration"/>
                </xs:choice>
                <xs:element minOccurs="0" ref="ani:et-al"/>
                <xs:element minOccurs="0" ref="ani:affiliation"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="author">
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="ani:personalnameType">
                    <xs:sequence>
                        <xs:element minOccurs="0" ref="ani:preferred-name"/>
                        <xs:element minOccurs="0" ref="ce:e-address"/>
                    </xs:sequence>
                    <xs:attribute name="auid"/>
                    <xs:attribute name="seq" use="required"/>
                    <xs:attribute name="type"/>
                    <xs:attribute name="orcid"/>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="preferred-name" type="ani:personalnameType"/>
    <xs:element name="collaboration">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="ce:indexed-name"/>
                <xs:element ref="ce:text"/>
            </xs:sequence>
            <xs:attribute name="seq" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="et-al">
        <xs:complexType/>
    </xs:element>
    <xs:element name="affiliation">
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="ani:affiliationType">
                    <xs:attribute name="afid"/>
                    <xs:attribute name="dptid"/>
                    <xs:attribute name="country"/>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="organization">
        <xs:complexType mixed="true">
            <xs:group minOccurs="0" maxOccurs="unbounded" ref="ani:richstring.data"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="address-part">
        <xs:complexType mixed="true">
            <xs:group minOccurs="0" maxOccurs="unbounded" ref="ani:richstring.data"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="city-group" type="xs:string"/>
    <xs:element name="city" type="xs:string"/>
    <xs:element name="state" type="xs:string"/>
    <xs:element name="postal-code">
        <xs:complexType mixed="true">
            <xs:attribute name="type">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="pre"/>
                        <xs:enumeration value="post"/>
                        <xs:enumeration value="zip"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="country">
        <xs:complexType>
            <xs:attribute name="iso-code" use="required"/>
        </xs:complexType>
    </xs:element>

    <!-- Correspondence Address elements -->

    <xs:element name="correspondence">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="ani:person"/>
                <xs:element minOccurs="0" ref="ani:affiliation"/>
                <xs:element minOccurs="0" ref="ce:e-address"/>
            </xs:sequence>
