            <xs:element ref="ani:reportnumber"/>
        </xs:sequence>
    </xs:complexType>
    <xs:element name="reportnumber" type="xs:string"/>

    <xs:element name="bib-text" type="xs:string"/>

    <!-- Patent elements -->

    <xs:element name="patent">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="ani:registrations"/>
                <xs:element minOccurs="0" ref="ani:localapplications"/>
                <xs:element minOccurs="0" ref="ani:prioapplications"/>
                <xs:element minOccurs="0" ref="ani:assignees"/>
                <xs:element minOccurs="0" ref="ani:designatedcountries"/>
                <xs:element minOccurs="0" ref="ani:ipc-codes"/>
                <xs:element minOccurs="0" ref="ani:inventors"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="registrations">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="ani:registration"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="registration" type="xs:string"/>
    <xs:element name="localapplications">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="ani:localapplication"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="localapplication" type="xs:string"/>
    <xs:element name="prioapplications">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="ani:prioapplication"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="prioapplication" type="xs:string"/>
    <xs:element name="assignees">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="ani:assignee"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="assignee" type="xs:string"/>
    <xs:element name="designatedcountries">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="ani:country"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="ipc-codes">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="ani:ipc-code"/>
            </xs:sequence>
        </xs:complexType>
