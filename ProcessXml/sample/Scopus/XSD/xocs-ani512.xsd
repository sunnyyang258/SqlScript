<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" targetNamespace="http://www.elsevier.com/xml/xocs/dtd" xmlns:ait="http://www.elsevier.com/xml/ani/ait"
           xmlns:ani="http://www.elsevier.com/xml/ani/ani" xmlns:xocs="http://www.elsevier.com/xml/xocs/dtd" xmlns:cto="http://www.elsevier.com/xml/cto/dtd" xmlns:ce="http://www.elsevier.com/xml/ani/common">

	<xs:import namespace="http://www.elsevier.com/xml/ani/ait" schemaLocation="xocs-ani512-ait.xsd"/>
	<xs:import namespace="http://www.elsevier.com/xml/ani/common" schemaLocation="xocs-ani512-ce.xsd"/>
	<xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xocs-ani512-xml.xsd"/>
	<xs:import namespace="http://www.elsevier.com/xml/ani/ani" schemaLocation="xocs-ani512-ani-ce.xsd"/>
	<xs:import namespace="http://www.elsevier.com/xml/cto/dtd" schemaLocation="xocs-ani512-cto.xsd"/>

	<xs:element name="doc" type="xocs:doc-tp"/>

	<xs:complexType name="doc-tp">
		<xs:sequence>
			<xs:element name="meta" type="xocs:meta-tp"/>
			<xs:element name="item" type="xocs:item-tp"/>
			<!-- xs:element ref="xocs:item"/ -->
		</xs:sequence>
		<xs:attribute name="content-type" type="xocs:content-type-tp" use="required"/>
		<xs:attribute name="dbname" type="xocs:dbname-tp" use="required"/>
	</xs:complexType>

	<xs:simpleType name="content-type-tp">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Abstract"/>
			<xs:enumeration value="journal"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="dbname-tp">
		<xs:restriction base="xs:string">
			<xs:enumeration value="scopusbase"/>
			<xs:enumeration value="econlit"/>
			<xs:enumeration value="psycinfo"/>
			<xs:enumeration value="medline"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="meta-tp">
		<xs:sequence>
			<xs:element name="eid" type="xocs:nonEmptyString" nillable="false"/>
			<xs:element name="oeid" type="xs:string"/>
			<xs:element name="doi" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="pmid" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="online-status" type="xocs:online-status-tp"/>
			<xs:element name="pui" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="pii" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="fulltext-eid" type="xocs:fulltext-eid-tp" minOccurs="0" maxOccurs="1"/>
            <xs:element name="sdfullavail" type="xs:integer"/>
			<xs:element name="issn" type="xs:string"/>
			<xs:element name="volume" type="xs:string"/>
			<xs:element name="issue" type="xs:string"/>
			<xs:element name="author-surname" type="xs:string"/>
			<xs:element name="author-fi" type="xs:string"/>
			<xs:element name="firstpage" type="xs:string"/>
			<xs:element name="lastpage" type="xs:string"/>
			<xs:element name="sort-year" type="xs:string"/>
			<xs:element name="sort-yyyymm" type="xs:string"/>
			<xs:element name="pub-year" type="xs:string" minOccurs="0" maxOccurs="1"/>
            <xs:element name="timestamp" type="xocs:timestamp-tp" nillable="false"/>
            <xs:element name="orig-load-date" type="xocs:date-tp" nillable="false"/>
			<xs:element name="datesort" type="xs:integer" nillable="false"/>
			<xs:element name="indexeddate" type="xocs:indexed-date-tp" />
			<xs:element name="absavail" type="xs:integer"/>
			<xs:element name="suppressdummy" type="xocs:yes-no-string-tp"/>
			<xs:element name="srctype" type="xs:string"/>
			<xs:element name="subjareas" type="xocs:subj-areas-tp" minOccurs="0"/>
			<xs:element name="srctitle" type="xs:string"/>
			<xs:element name="countries" type="xocs:countries-tp" minOccurs="0"/>
			<xs:element name="languages" type="xocs:languages-tp" minOccurs="0"/>
			<xs:element name="prefnameauids" type="xocs:prefnameauids-tp" minOccurs="0"/>
            <xs:element ref="cto:doctype" maxOccurs="unbounded"/>
			<xs:element ref="cto:group-id"/>
			<xs:element ref="cto:ref-id" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="cto:unique-author" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="cto:unique-auth-count"/>
			<xs:element name="refeidnss" type="xs:string" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

	<!-- fulltext-eid score -->
	<xs:complexType name="fulltext-eid-tp">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="score" use="required" type="xs:string"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<!-- the following simple type states that the content must be a string with atleast one non Whitespace character -->
	<xs:simpleType name="nonEmptyString">
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="online-status-tp">
		<xs:restriction base="xs:string">
			<xs:enumeration value="available"/>
			<xs:enumeration value="unavailable"/>
		</xs:restriction>
	</xs:simpleType>

    <!-- ========== Citation types ==========
        ab  Abstract Report
        ar  Article
        ba
        bk  Book
        br  Book Review
        bz  Business Article
        cb  Conference Abstract
        ch  Chapter
        cp  Conference Paper
        cr  Conference Review
        di  Dissertation
        ed  Editorial
        er  Erratum
        ip  Article in Press
        le  Letter
        no  Note
        pa  Patent
        pr  Press Release
        re  Review
        rf
        rp  Report
        sh  Short Survey
        wp  Working Paper
    -->
    <xs:simpleType name="citationtype.att">
        <xs:restriction base="xs:token">
            <xs:enumeration value="ab"/>
            <xs:enumeration value="ar"/>
            <xs:enumeration value="ba"/>
            <xs:enumeration value="bk"/>
            <xs:enumeration value="br"/>
            <xs:enumeration value="bz"/>
            <xs:enumeration value="cb"/>
            <xs:enumeration value="ch"/>
            <xs:enumeration value="cp"/>
            <xs:enumeration value="cr"/>
            <xs:enumeration value="di"/>
            <xs:enumeration value="ed"/>
            <xs:enumeration value="er"/>
            <xs:enumeration value="ip"/>
            <xs:enumeration value="le"/>
            <xs:enumeration value="no"/>
            <xs:enumeration value="pa"/>
            <xs:enumeration value="pr"/>
            <xs:enumeration value="re"/>
            <xs:enumeration value="rf"/>
            <xs:enumeration value="rp"/>
            <xs:enumeration value="sh"/>
            <xs:enumeration value="wp"/>
        </xs:restriction>
    </xs:simpleType>

	
	<!-- ========== GenCitation types ==========
        bk  Book
        ch  Chapter
        co  Conference Proceeding
        cp  Conference Paper
        di  Dissertation
        ep  E-print
        ot  Other
        pa  Patent
        st  Standard
        tr  Technical Report
    -->
	<xs:simpleType name="gencitationtype.att">
		<xs:restriction base="xs:token">
			<xs:enumeration value="bk"/>
			<xs:enumeration value="ch"/>
			<xs:enumeration value="co"/>
			<xs:enumeration value="cp"/>
			<xs:enumeration value="di"/>
			<xs:enumeration value="ep"/>
			<xs:enumeration value="ot"/>
			<xs:enumeration value="pa"/>
			<xs:enumeration value="st"/>
			<xs:enumeration value="tr"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- ========== Sequencebank types ==========
    -->
	<xs:simpleType name="sequencebanktype.att">
		<xs:restriction base="xs:token">
			<xs:enumeration value="DDBJ"/>
			<xs:enumeration value="EMBL"/>
			<xs:enumeration value="GENBANK"/>
			<xs:enumeration value="PIR"/>
			<xs:enumeration value="SWISSPROT"/>
			<xs:enumeration value="UNNAMED"/>
		</xs:restriction>
	</xs:simpleType>

    <!-- ========== Descriptor types ==========
    -->
    <xs:simpleType name="descriptortype.att">
        <xs:restriction base="xs:token">
            <xs:enumeration value="AUD"/>
            <xs:enumeration value="AUT"/>
            <xs:enumeration value="BLS"/>
            <xs:enumeration value="BTC"/>
            <xs:enumeration value="CBA"/>
            <xs:enumeration value="CBB"/>
            <xs:enumeration value="CBC"/>
            <xs:enumeration value="CBE"/>
            <xs:enumeration value="CCV"/>
            <xs:enumeration value="CFL"/>
            <xs:enumeration value="CLU"/>
            <xs:enumeration value="CMH"/>
            <xs:enumeration value="CNC"/>
            <xs:enumeration value="CSL"/>
            <xs:enumeration value="CSS"/>
            <xs:enumeration value="CSW"/>
            <xs:enumeration value="CTB"/>
            <xs:enumeration value="CTC"/>
            <xs:enumeration value="CTN"/>
            <xs:enumeration value="DID"/>
            <xs:enumeration value="DRA"/>
            <xs:enumeration value="DRG"/>
            <xs:enumeration value="ECN"/>
            <xs:enumeration value="FDE"/>
            <xs:enumeration value="GDE"/>
            <xs:enumeration value="GLY"/>
            <xs:enumeration value="KWD"/>
            <xs:enumeration value="MEA"/>
            <xs:enumeration value="MED"/>
            <xs:enumeration value="MGS"/>
            <xs:enumeration value="MKW"/>
            <xs:enumeration value="MLA"/>
            <xs:enumeration value="MMS"/>
            <xs:enumeration value="MSC"/>
            <xs:enumeration value="MSF"/>
            <xs:enumeration value="MSH"/>
            <xs:enumeration value="MUD"/>
            <xs:enumeration value="OZR"/>
            <xs:enumeration value="PCV"/>
            <xs:enumeration value="REA"/>
            <xs:enumeration value="RGI"/>
            <xs:enumeration value="SID"/>
            <xs:enumeration value="SPC"/>
            <xs:enumeration value="SPO"/>
            <xs:enumeration value="SUA"/>
            <xs:enumeration value="TRP"/>
            <xs:enumeration value="WDE"/>
            <xs:enumeration value="SCO"/>
            <xs:enumeration value="SCT"/>
            <xs:enumeration value="IBT"/>
            <xs:enumeration value="IGI"/>
            <xs:enumeration value="SCC"/>
            <xs:enumeration value="MDV"/>            
        </xs:restriction>
    </xs:simpleType>

    <!-- ========== Classification types ==========
    -->
    <xs:simpleType name="classificationtype.att">
        <xs:restriction base="xs:token">
            <xs:enumeration value="ASJC"/>
            <xs:enumeration value="AUTARC-CONFIGURATION"/>
            <xs:enumeration value="CABSCLASS"/>
            <xs:enumeration value="CBNBGEO"/>
            <xs:enumeration value="CBNBSCOPE"/>
            <xs:enumeration value="CBNBSECTOR"/>
            <xs:enumeration value="CBNBSIC"/>
            <xs:enumeration value="CONFIGURATION-VALUE"/>
            <xs:enumeration value="CPXCLASS"/>
            <xs:enumeration value="EMCLASS"/>
            <xs:enumeration value="ESBD"/>
	    <xs:enumeration value="COMPASSCLASS"/>
            <xs:enumeration value="ENCOMPASSCLASS"/>
            <xs:enumeration value="FLXCLASS"/>
            <xs:enumeration value="GEOCLASS"/>
            <xs:enumeration value="PATAUTO"/>
            <xs:enumeration value="PATENT-FAMILY"/>
            <xs:enumeration value="PATMANU"/>
            <xs:enumeration value="REAXYS-PRIORITY"/>
            <xs:enumeration value="SATURATION"/>
            <xs:enumeration value="SUBJECT"/>
            <xs:enumeration value="SUPPORT-INFORMATION"/>
            <xs:enumeration value="WTACLASS"/>
	    <xs:enumeration value="SUBJABBR"/>
        </xs:restriction>
    </xs:simpleType>

	<!-- ========== Tradename types ==========
    -->
	<xs:simpleType name="tradenametype.att">
		<xs:restriction base="xs:token">
			<xs:enumeration value="TNM"/>
			<xs:enumeration value="TNV"/>
			<xs:enumeration value="TNW"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- ========== Manufacturer types ==========
    -->
	<xs:simpleType name="manufacturertype.att">
		<xs:restriction base="xs:token">
			<xs:enumeration value="MNF"/>
			<xs:enumeration value="MNV"/>
			<xs:enumeration value="MNW"/>
		</xs:restriction>
	</xs:simpleType>

    <!-- ========== Chemical source types ==========
    -->
    <xs:simpleType name="chemicalsourcetype.att">
        <xs:restriction base="xs:token">
            <xs:enumeration value="esbd"/>
            <xs:enumeration value="nlm"/>
            <xs:enumeration value="cbnb"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- ========== Source Types ==========
        b  Book
        d  Trade Journal
        j  Journal
        k  Book Series
        m  Multi-volume Reference Works
        p  Conference Proceeding
        r  Report
        n  Newsletter
        w  Newspaper
    -->
    <xs:simpleType name="sourcetype.att">
        <xs:restriction base="xs:token">
            <xs:enumeration value="b"/>
            <xs:enumeration value="d"/>
            <xs:enumeration value="j"/>
            <xs:enumeration value="k"/>
            <xs:enumeration value="m"/>
            <xs:enumeration value="p"/>
            <xs:enumeration value="r"/>
            <xs:enumeration value="n"/>
            <xs:enumeration value="w"/>
        </xs:restriction>
    </xs:simpleType>

	<!-- ========== Contributor Roles ==========
        auth  author
        comp  compiler
        edit  editor
        illu  illustrator
        phot  photographer
        publ  publisher
        revi  reviewer
        tran  translator
    -->
	<xs:simpleType name="contributorrole.att">
		<xs:restriction base="xs:token">
			<xs:enumeration value="auth"/>
			<xs:enumeration value="comp"/>
			<xs:enumeration value="edit"/>
			<xs:enumeration value="illu"/>
			<xs:enumeration value="phot"/>
			<xs:enumeration value="publ"/>
			<xs:enumeration value="revi"/>
			<xs:enumeration value="tran"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="yesno.att">
		<xs:restriction base="xs:token">
			<xs:enumeration value="y"/>
			<xs:enumeration value="n"/>
		</xs:restriction>
	</xs:simpleType>

    <xs:group name="pubdateType">
        <xs:sequence>
            <xs:element name="year" type="xs:string" minOccurs="0" form="unqualified"/>
            <xs:element name="month" type="xs:string" minOccurs="0" form="unqualified"/>
            <xs:element name="day" type="xs:string" minOccurs="0" form="unqualified"/>
            <xs:element name="date-text" type="xocs:date-text-tp" minOccurs="0" form="unqualified"/>
        </xs:sequence>
    </xs:group>
	<!--xs:group name="pubdateType">
		<xs:sequence>
			<xs:sequence minOccurs="0" maxOccurs="1">
				<xs:element name="year" type="xs:string" form="unqualified"/>
				<xs:choice minOccurs="0">
					<xs:sequence>
						<xs:element name="month" type="xs:string" form="unqualified"/>
						<xs:element name="day" type="xs:string" minOccurs="0" form="unqualified"/>
					</xs:sequence>
				</xs:choice>
			</xs:sequence>
			<xs:element name="date-text" type="xocs:date-text-tp" form="unqualified"/>
		</xs:sequence>
	</xs:group-->

	<xs:complexType name="date-text-tp" mixed="true">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="xfab-added" use="optional" type="xs:boolean"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<xs:complexType name="personalnameType">
		<xs:sequence>
			<xs:element minOccurs="0" ref="ce:initials"/>
			<xs:element ref="ce:indexed-name"/>
			<xs:choice minOccurs="0">
				<xs:sequence>
					<xs:element minOccurs="0" ref="ce:degrees"/>
					<xs:element ref="ce:surname"/>
					<xs:element minOccurs="0" ref="ce:given-name"/>
					<xs:element minOccurs="0" ref="ce:suffix"/>
				</xs:sequence>
				<xs:element name="nametext" type="xs:string" form="unqualified"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="affiliationType">
		<xs:sequence>
			<xs:choice>
				<xs:element ref="ce:text"/>
				<xs:sequence>
					<xs:element name="organization" type="xocs:organization-tp" minOccurs="0" maxOccurs="unbounded" form="unqualified"/>
					<xs:element name="address-part" type="xocs:address-part-tp" minOccurs="0" form="unqualified"/>
					<xs:choice minOccurs="0">
						<xs:element name="city-group" type="xs:string" form="unqualified"/>
						<xs:sequence>
							<xs:element name="city" type="xs:string" minOccurs="0" form="unqualified"/>
							<xs:element name="state" type="xs:string" minOccurs="0" form="unqualified"/>
							<xs:element name="postal-code" type="xocs:postal-code-tp" minOccurs="0" maxOccurs="unbounded" form="unqualified"/>
						</xs:sequence>
					</xs:choice>
				</xs:sequence>
			</xs:choice>
			<xs:element name="country" type="xs:string" minOccurs="0" maxOccurs="1" form="unqualified"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="classification-tp" mixed="true">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="classification-code" type="xs:string" form="unqualified"/>
			<xs:element name="classification-description" type="xs:string" form="unqualified"/>
		</xs:choice>
		<xs:attribute name="weight">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="-"/>
					<xs:enumeration value="?"/>
					<xs:enumeration value="+"/>
					<xs:enumeration value="++"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>

	<!-- ========== Wrapper and process info ========== -->

	<xs:complexType name="item-tp">
		<xs:sequence>
			<xs:element name="item" type="xocs:origItem-tp" form="unqualified"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="origItem-tp">
		<xs:sequence>
			<xs:element ref="ait:process-info"/>
			<xs:element name="bibrecord" type="xocs:bibrecord-tp" form="unqualified"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="bibrecord-tp">
		<xs:sequence>
			<xs:element name="item-info" type="xocs:item-info-tp" form="unqualified"/>
			<xs:element name="itemfilelinks" type="xocs:itemfilelinks-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="head" type="xocs:head-tp" form="unqualified"/>
			<xs:element name="tail" type="xocs:tail-tp" minOccurs="0" form="unqualified"/>
		</xs:sequence>
	</xs:complexType>

	<!-- ========== Bibliographic record ========== -->

	<xs:complexType name="item-info-tp">
		<xs:sequence>
			<xs:element name="copyright" type="xocs:copyright-tp" maxOccurs="unbounded" form="unqualified"/>
			<xs:element name="itemidlist" type="xocs:itemidlist-tp" form="unqualified"/>
			<xs:element name="history" type="xocs:history-tp" form="unqualified"/>
			<xs:element name="dbcollection" type="xs:string" minOccurs="0" maxOccurs="unbounded" form="unqualified"/>
			<xs:element name="external-source" type="xs:string" minOccurs="0" form="unqualified"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="itemfilelinks-tp">
		<xs:sequence>
			<xs:element name="itemfilelink" type="xocs:itemfilelink-tp" maxOccurs="unbounded" form="unqualified"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="itemfilelink-tp">
		<xs:attribute name="href" use="required"/>
		<xs:attribute name="MIME-type"/>
	</xs:complexType>

	<xs:complexType name="copyright-tp" mixed="true">
		<xs:attribute name="type"/>
	</xs:complexType>

	<xs:complexType name="itemidlist-tp">
		<xs:sequence>
			<xs:element ref="ce:pii" minOccurs="0" maxOccurs="1"/>
			<xs:element ref="ce:doi" minOccurs="0" maxOccurs="1"/>
			<xs:element name="itemid" type="xocs:itemid-tp" maxOccurs="unbounded" form="unqualified"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="itemid-tp" mixed="true">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="idtype" type="xs:NMTOKEN" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<!-- Item history elements -->

	<xs:complexType name="history-tp">
		<xs:sequence>
			<xs:element name="date-created" type="xocs:date-created-tp" form="unqualified"/>
			<xs:element name="date-completed" type="xocs:date-completed-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="date-revised" type="xocs:date-revised-tp" minOccurs="0" maxOccurs="unbounded" form="unqualified"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="date-created-tp">
		<xs:attributeGroup ref="ce:date.att"/>
	</xs:complexType>
	<xs:complexType name="date-completed-tp">
		<xs:attributeGroup ref="ce:date.att"/>
	</xs:complexType>

	<xs:complexType name="date-revised-tp">
		<xs:simpleContent>
			<xs:extension base="xocs:date-content-tp">
				<xs:attribute name="year" type="xs:NMTOKEN" use="required"/>
				<xs:attribute name="month" type="xs:NMTOKEN" use="required"/>
				<xs:attribute name="day" type="xs:NMTOKEN" use="required"/>
				<xs:attribute name="timestamp" type="xs:dateTime" use="optional"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<xs:simpleType name="date-content-tp">
		<xs:restriction base="xs:string">
			<xs:pattern value="(.{0})|([0-9]{8})"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- ========== head ========== -->

	<xs:complexType name="head-tp">
		<xs:sequence>
